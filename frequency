
//given a sorted array and a number x count how many times x occured//


#include <iostream>
using namespace std;
int main()
{
    int arr[5];
    int count = 0;
    cout << "Enter 5 sorted elements: ";
    for (int i = 0; i < 5; i++) {
        cin >> arr[i];
    }
    cout << "Enter the number to find its frequency: ";
    int x;
    cin >> x;
    for(int i = 0; i < 5; i++) {
        if (arr[i] == x) {
            count++;
        }
    }
    cout << "Frequency of " << x << ": " << count << endl;
}
